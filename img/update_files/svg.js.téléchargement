    $(".base_box .px").click(function() {
        var px = $(this).attr('data');
        var svgXml = 'data:image/svg+xml;base64,' + window.btoa(unescape(encodeURIComponent($('.svg_img .img').html())));
        base(px, svgXml,1);
    });

