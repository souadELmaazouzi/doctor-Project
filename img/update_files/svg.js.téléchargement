        var checkbox = '';
        $(".downloadbox .format .ck input[type=checkbox]:checked").each(function() {
            checkbox = checkbox + '' + $(this).attr("id") + '|';
        });
        if (checkbox == '') {
            sAlert('Please select a icon format!');
            return (false);
